{
  "expo": {
    "name": "FORCE",
    "slug": "force",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "force",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.force.performance.app",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSAppTransportSecurity": {
          "NSExceptionDomains": {
            "api.utrack.irfanemreutkan.com": {
              "NSExceptionAllowsInsecureHTTPLoads": true,
              "NSIncludesSubdomains": true
            }
          }
        },
        "NSHealthShareUsageDescription": "This app needs access to your step count to display your daily activity.",
        "NSHealthUpdateUsageDescription": "This app needs access to update your health data."
      }
    },
    "android": {
      "package": "com.force.performance.app",
      "adaptiveIcon": {
        "backgroundColor": "#020205",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-dev-client",
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#020205",
          "dark": {
            "backgroundColor": "#020205"
          }
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "usesCleartextTraffic": true,
            "networkSecurityConfig": "./android/app/src/main/res/xml/network_security_config.xml",
            "debuggable": true
          }
        }
      ],
      "./plugins/withImmersiveMode",
      "./plugins/withHealthKit",
      "./plugins/withGoogleFit",
      [
        "@react-native-google-signin/google-signin",
        {
          "iosUrlScheme": "overridden-by-app.config.js"
        }
      ],
      "expo-apple-authentication",
      "expo-secure-store",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "a8c6e03b-2990-461b-8cd9-582ddeec2e5b"
      }
    },
    "owner": "emreutkan"
  }
}
